{
  "name": "zensen-generator",
  "version": "0.0.1",
  "description": "Generates a zensen app",
  "main": "dist/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && babel src -d dist/src --copy-files",
    "watch": "nodemon --exec \"npm run build\"",
    "lint": "eslint .",
    "test": "NODE_ENV=test mocha --require @babel/register --timeout 300000 --exit test test/**/*.test.js",
    "lint:fix": "eslint --fix ."
  },
  "nodemonConfig": {
    "ignore": [
      "dist/**"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "express": "^4.18.2",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.4",
    "sequelize": "^5.21.10",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2"
  },
  "peerDependencies": {
    "express": "^4.18.2",
    "sequelize": "^5.21.10"
  }
}
